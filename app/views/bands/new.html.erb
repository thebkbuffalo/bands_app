Add A Band

<%= form_for @band do |f| %>
  <%= f.label :name %><br/>
  <%= f.text_field :name %><br/>
  <%= f.label :genre %><br/>
  <%= f.text_field :genre %><br/>
  <%= f.label :explicit_lyrics %><br/>
  <%= f.radio_button :explicit_lyrics, :true %>True<br/>
  <%= f.radio_button :explicit_lyrics, :false %>False<br/>
  <%= f.submit %>
<% end %>
